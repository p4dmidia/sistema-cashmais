{
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/debug-vars",
      "destination": "/api/debug-vars"
    },
    {
      "source": "/api",
      "destination": "/api/proxy"
    },
    {
      "source": "/api/",
      "destination": "/api/proxy"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy?path=$1"
    },
    {
      "source": "/:path((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
